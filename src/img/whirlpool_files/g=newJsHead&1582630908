function P7_AP4remClass(ob,cl){if(ob){var cc,nc;cc=ob.className;cl=cl.replace('-','\-');var re=new RegExp('\\b'+cl+'\\b');if(re.test(cc)){nc=cc.replace(re,'');nc=nc.replace(/\s+/g,' ');nc=nc.replace(/\s$/,'');nc=nc.replace(/^\s/,'');ob.className=nc;}}}
function P7_AP4anim(tp,t,b,c,d){if(tp=='linear'){return(c*(t/d))+b;}else if(tp=='quad'){if((t/=d/2)<1){return c/2*t*t+b;}else{return-c/2*((--t)*(t-2)-1)+b;}}else if(tp=='inquad'){var tt=t/d;return c*(tt)*(tt)+b;}else if(tp=='bounce'){if((t/=d)<(1/2.75)){return c*(7.5625*t*t)+b;}else if(t<(2/2.75)){return c*(7.5625*(t-=(1.5/2.75))*t+0.75)+b;}else if(t<(2.5/2.75)){return c*(7.5625*(t-=(2.25/2.75))*t+0.9375)+b;}else{return c*(7.5625*(t-=(2.625/2.75))*t+0.984375)+b;}}}
