if(!cl&&Math.abs(swl)>4){evt.preventDefault();}}else{P7_OPMtchCancel();}}
function P7_OPMtchEnd(evt){if(p7OPM.tchEl){var swl,el,dir,cl;el=p7OPM.tchEl;p7OPM.tchEl=null;swl=p7OPM.curX-p7OPM.startX;cl=(p7OPM.curX===0&&p7OPM.curY===0);if(!cl&&Math.abs(swl)>70){if(swl<0){dir='left';}else{dir='right';}
