P7_MBXoverlay(tD,'open');tD.style.top=P7_MBXgetScroll()+'px';tD.style.left='0px';P7_MBXsetClass(tD,'active');}}
function P7_MBXcloseBox(){var tD=p7MBX.currentShow;tD.mbxStatus='closed';tD.mbxShowResume=false;P7_MBXremClass(document.getElementsByTagName('BODY')[0],'p7mbx-running');if(tD.mbxShowTimer){clearTimeout(tD.mbxShowTimer);}
