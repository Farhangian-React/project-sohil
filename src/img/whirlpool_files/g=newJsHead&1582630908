return v;}
function P7_SMLgetScrollBody(){var y,el=document.documentElement;if(el){y=el.scrollTop;el.scrollTop+=1;if(el.scrollTop==y){el=document.body;}
else{el.scrollTop-=1;}}
else{el=document.body;}
