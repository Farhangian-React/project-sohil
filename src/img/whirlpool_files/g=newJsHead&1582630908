function P7_MXMfinAnim(ul,ac){var st=ul.mxmTrigger.mxmState;if((ac=='open'&&st=='open')||(ac=='closed'&&st=='closed')){ul.style.overflow='visible';ul.style[p7MXM.prf+'transition']=null;ul.style[p7MXM.prf+'transform']='none';ul.style.clip='auto';if(ac=='closed'){ul.style.display='none';ul.style.opacity=1;}}}
function P7_MXMclose(a,bp){var i,op,tB,sD,dur,sc;if(a.mxmState=='closed'){return;}
tB=document.getElementById(a.mxmDiv);op=tB.mxmOpt[1];dur=tB.mxmOpt[0];if(op>0&&P7_MXMmenuMode(tB)=='accordion'){op=99;}
