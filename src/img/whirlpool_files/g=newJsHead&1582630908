function P7_SOMtoolbar(d,ac,bp){var tB,tBR,fr,tr,prop,an,un='%',br,bf;tB=document.getElementById(d);if(tB){tBR=tB.somTBR;}
else{return false;}
if((ac=='open'&&tB.somState=='opened')||(ac=='closed'&&tB.somState=='closed')){return false;}
if(tB&&tBR){if((ac!='close'&&ac!='open')||!ac){ac=(tBR.somState=='closed')?'open':'closed';}
an=(bp==1)?0:tB.p7opt[3];if(ac=='open'){tB.somState='opened';P7_SOMchangeClass(tB,'closed','opened');tBR.somState='opened';P7_SOMchangeClass(tBR,'closed','opened');tB.somUL.somState='opened';P7_SOMchangeClass(tB.somUL,'closed','opened');if(tB.p7opt[2]==1){tr=0;fr=parseInt(P7_SOMgetStyle(tB.somUL,'left'),10);prop='left';}
