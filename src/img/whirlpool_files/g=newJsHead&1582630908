else{tB.somUL.style[prop]=tr+un;tB.style.display='none';}}}
return false;}
function P7_SOMclick(a,bp){var wH,tB,cnv=false,m=false;wH=window.location.href;if(a.href!=wH&&a.href!=wH+'#'){if(a.href.toLowerCase().indexOf('javascript:')==-1){m=true;}}
