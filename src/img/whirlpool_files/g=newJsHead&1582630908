function P7_ISMgetByAttribute(att,cls){var i,nL=[],aT,rS=[],cl;if(document.querySelectorAll){nL=document.querySelectorAll('*['+att+']');}else{if(typeof(document.getElementsByClassName)!='function'){aT=document.getElementsByTagName('DIV');for(i=0;i<aT.length;i++){cl=aT[i].className;if(cl&&cl.indexOf(cls)>-1){rS[rS.length]=aT[i];}}}else{rS=document.getElementsByClassName(cls);}
