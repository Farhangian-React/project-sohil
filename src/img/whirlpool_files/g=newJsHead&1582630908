function P7_MGMfade(d,tp){var i,el,tC,tA,op,et,cet,m=false;el=document.getElementById(d);et=P7_MGMgetTime(el.mgmStartTime);if(et>=el.mgmDuration){et=el.mgmDuration;m=true;}
