function P7_MGManimator(d,op){var i,tB,tA,tS,et,nv,m=false;tB=document.getElementById(d);et=P7_MGMgetTime(tB.p7animStartTime);if(et>=tB.p7animDuration){et=tB.p7animDuration;}
