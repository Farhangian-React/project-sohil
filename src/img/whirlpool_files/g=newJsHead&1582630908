function P7_OPManim(tp,t,b,c,d){if(tp=='quad'){return-c*(t/=d)*(t-2)+b;}else if(tp=='cubic'){return c*((t=t/d-1)*t*t+1)+b;}else{return(c*(t/d))+b;}}
function P7_OPMgetTime(st){var d=new Date();return d.getTime()-st;}
function P7_OPMste(el){var st,t,bD,sb,se;bD=document.body;bD.p7AnimRunning=false;if(bD.p7STEanim){clearInterval(bD.p7STEanim);}
