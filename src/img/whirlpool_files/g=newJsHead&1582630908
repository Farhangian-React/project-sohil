function i8_serializeForm(form){var field;var query="";if(typeof form==="object"&&form.nodeName==="FORM"){for(var i=form.elements.length-1;i>=0;i--){field=form.elements[i];if(field.name&&field.type!=="file"&&field.type!=="reset"){if(field.type==="select-multiple"){for(var j=form.elements[i].options.length-1;j>=0;j--){if(field.options[j].selected){query+="&"+field.name+"="+encodeURIComponent(field.options[j].value).replace(/%20/g,"+");}}}
