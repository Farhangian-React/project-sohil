function P7_AMMgetTime(st){var d=new Date();return d.getTime()-st;}
function P7_AMManim(tp,t,b,c,d){if(tp=='quad'){return-c*(t/=d)*(t-2)+b;}else if(tp=='cubic'){return c*((t=t/d-1)*t*t+1)+b;}else{return(c*(t/d))+b;}}
