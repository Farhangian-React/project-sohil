function P7_SOPgetIEver(){var j,k,v=-1,nv;nv=navigator.userAgent.toLowerCase();j=nv.indexOf("msie");if(j>-1){v=parseFloat(nv.substring(j+4,j+8));if(document.documentMode){v=document.documentMode;}
