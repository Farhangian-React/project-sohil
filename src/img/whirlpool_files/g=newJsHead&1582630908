an=tB.p7opt[5];dur=tB.p7opt[6];tB.style.visibility='hidden';if(tB.p7opt[1]==1){tB.style.right='0px';}else{tB.style.left='0px';}
tW=tB.sopWrapper;if(tB.sopFrames){P7_SOPframes(tB,'on');}
if(tB.p7opt[14]==1){tW.scrollTop=0;}
