function P7_OPMtchMove(evt){var tch,el,x,y,swl,cl;evt=(evt)?evt:event;tch=(evt.touches&&evt.touches[0])?true:false;if(p7OPM.tchEl){el=p7OPM.tchEl;if(tch&&evt.touches.length>1){P7_OPMtchCancel(evt);return;}
