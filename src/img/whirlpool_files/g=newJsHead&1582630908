if(typeof(p7STT)=='object'){if(p7STT.body&&p7STT.body.p7AnimRunning){p7STT.body.p7AnimRunning=false;clearInterval(p7STT.body.p7STTanim);}}
st=p7AMM.body.scrollTop;t=st+el.getBoundingClientRect().top+1;tf=parseInt(a.getAttribute('data-top-offset'),10);if(tf&&tf!==''){t-=tf;}
tf=a.getAttribute('data-top-offset-id');if(tf&&tf!==''){tb=document.getElementById(tf);if(tb){t-=tb.offsetHeight;}}
