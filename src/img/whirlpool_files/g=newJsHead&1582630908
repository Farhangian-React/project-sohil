function P7_OPMgetMode(tB){var cl,md='normal';if(P7_OPMgetStyle(tB,'max-height')=='700777px'){md='accordion';}else{cl=tB.className;if(cl&&cl!==''&&cl.indexOf('opm-v-tabs')>-1){md='vtabs';}}
