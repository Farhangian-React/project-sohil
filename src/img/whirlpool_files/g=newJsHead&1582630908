function P7_AP4changeClass(ob,clf,clt){P7_AP4remClass(ob,clf);P7_AP4setClass(ob,clt);}
function P7_AP4setClass(ob,cl){if(ob){var cc,nc,r=/\s+/g;cc=ob.className;nc=cl;if(cc&&cc.length>0){if(cc.indexOf(cl)==-1){nc=cc+' '+cl;}else{nc=cc;}}
