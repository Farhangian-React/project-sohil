function P7_SMLremClass(ob,cl){if(ob){var cc,nc;cc=ob.className;if(cc&&cc.indexOf(cl>-1)){nc=cc.replace(cl,'');nc=nc.replace(/\s+/g,' ');nc=nc.replace(/\s$/,'');nc=nc.replace(/^\s/,'');ob.className=nc;}}}
