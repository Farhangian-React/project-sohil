function P7_OPMctrl(dv,ac){return P7_OPMcontrol(dv,ac);}
function P7_OPMcontrol(dv,ac,bp,tch){var i,a,tB,cs,ts,op,sn,tm=0,rs=false,m=false;tB=document.getElementById(dv);if(tB&&tB.opmTabs){if(tB.opmShowTimer){clearTimeout(tB.opmShowTimer);}
