function P7_MXMshutAll(bp){var i,tB;for(i=0;i<p7MXM.ctl.length;i++){tB=p7MXM.ctl[i];if(tB&&(!bp||bp!=tB.id)){P7_MXMshut(tB.id);}}}
function P7_MXMbody(evt){evt=(evt)?evt:event;var m=true,pp=(evt.fromElement)?evt.fromElement:evt.target;while(pp){if(pp&&pp.id&&typeof(pp.id)=='string'&&pp.id.indexOf('p7MXM')===0){m=false;break;}
