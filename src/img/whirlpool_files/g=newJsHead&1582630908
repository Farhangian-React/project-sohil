if(cb&&typeof(cb)==="function"){cb.call(el);}}}
function P7_SMLsetClass(ob,cl){if(ob){var cc,nc,r=/\s+/g;cc=ob.className;nc=cl;if(cc&&cc.length>0){if(cc.indexOf(cl)==-1){nc=cc+' '+cl;}
else{nc=cc;}}
