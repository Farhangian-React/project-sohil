sD.style.overflow='hidden';sD.style.opacity=(op==3)?0:1;if(sD.mxmLevel==1||a.mxmMaxi){sD.style.clip='rect(0px auto 0px 0px)';}else{if(sD.mxmSubPos=='right'){sD.style.clip='rect(0px 0px auto 0px)';}else{sD.style.clip='rect(0px auto auto '+sD.offsetWidth+'px)';}}
