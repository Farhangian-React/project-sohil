if(m){el.p7AnimRunning=false;clearInterval(el.p7AMManim);if(cb&&typeof(cb)==="function"){cb.call(el);}}}
function P7_AMMscrollToElement(a){var st,dy,op,el,t,tf,h,tb,tD;h=a.hash;if(!h||h===''){return false;}else{h=h.replace('#','');}
el=document.getElementById(h);if(!el){return false;}
