st=parseInt(p7SML.body.scrollTop,10);var wh=window.innerHeight||document.documentElement.clientHeight||document.body.clientHeight;var tt=el.getBoundingClientRect().top;var nst=0;if(tt<p7SML.bodyRestoreOffset){nst=st+tt-(wh/2);}
