function P7_OPMassignSwipe(el){var fn=function(evt){var x,y,tch,tB,md;evt=(evt)?evt:event;tch=(evt.touches&&evt.touches[0])?true:false;if(tch&&evt.touches.length>1){P7_OPMtchCancel(evt);return;}
