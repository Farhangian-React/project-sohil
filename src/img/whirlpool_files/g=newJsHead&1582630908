function P7_SOPcontrol(dv,ac){var i,a;for(i=0;i<p7SOP.ctl.length;i++){if(dv=='all'&&ac=='close'){if(p7SOP.ctl[i].sopState!='closed'){P7_SOPclose(p7SOP.ctl[i]);}}else if(p7SOP.ctl[i].id==dv){if(ac=='close'){P7_SOPclose(p7SOP.ctl[i]);break;}else if(ac=='open'){P7_SOPopen(p7SOP.ctl[i]);break;}else if(ac=='trig'){if(p7SOP.ctl[i].sopState=='open'){P7_SOPclose(p7SOP.ctl[i]);break;}else{P7_SOPopen(p7SOP.ctl[i]);break;}}}}
return false;}
function P7_SOPopen(tB){var an,tp,tW,dur;if(!tB||tB.sopState=='open'||!tB.sopWrapper){return;}
if(tB.sopAnimC){clearTimeout(tB.sopAnimC);}
