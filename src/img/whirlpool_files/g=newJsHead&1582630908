function P7_SOPsetClass(ob,cl){if(ob){var cc,nc,r=/\s+/g;cc=ob.className;nc=cl;if(cc&&cc.length>0){if(cc.indexOf(cl)==-1){nc=cc+' '+cl;}else{nc=cc;}}
