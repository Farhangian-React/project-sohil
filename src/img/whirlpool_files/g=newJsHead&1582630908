t=st+el.getBoundingClientRect().top;t-=el.offsetHeight;if(p7OPM.defAnim==1){bD.p7animType='quad';bD.p7animStartVal=st;bD.p7animCurrentVal=bD.p7animStartVal;bD.p7animFinishVal=t;bD.p7animStartTime=P7_OPMgetTime(0);bD.p7animDuration=p7OPM.defDuration;if(!bD.p7AnimRunning){bD.p7AnimRunning=true;bD.p7STEanim=setInterval(function(){P7_OPMsteA(bD);},p7OPM.animDelay);}}else{bD.body.scrollTop=t;document.documentElement.scrollTop=t;if(typeof(P7_STTrsz)=='function'){P7_STTrsz();}}
return false;}
